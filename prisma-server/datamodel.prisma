type User {
  id: ID! @unique
  email: String! @unique
  password: String
  name: String!
  companyName: String
  mobile: String
  interactionSessionId: String
  interactions: String
  loginWith: String
  disclaimer: Boolean! @default(value: "false")
  freeCoffee: Boolean! @default(value: "false")
  verified: Boolean! @default(value: "false")
  deactivated: Boolean! @default(value: "false")
}
